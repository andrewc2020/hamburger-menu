{
  "name": "with-playwright-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:e2e": "playwright test",
    "build:css": "postcss styles/tailwind.css -o styles/main.css",
    "build:watch": "postcss styles/tailwind.css -o styles/main.css --watch",
    "prod:build": "NODE_ENV=production npm run build:css && npm run build"
  },
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "next": "12.0.1",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@playwright/test": "^1.16.2",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.1"
  }
}
